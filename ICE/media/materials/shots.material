material machinegunFire
{
	technique
	{
		pass
		{
			
			lighting off
			scene_blend add
			depth_write off
			diffuse vertexcolour

			point_sprites on
			point_size 2
			point_size_attenuation on
		
			
			texture_unit
			{
				texture machinegunFire.png
			}
		}
	}
}
