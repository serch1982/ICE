material misilSmoke
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour

			texture_unit
			{
				texture misilSmoke.png
				tex_address_mode clamp
			}
		}
	}
}